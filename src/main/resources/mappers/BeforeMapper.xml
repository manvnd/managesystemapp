<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.managesystem.managesystempackage.repository.before.BeforeRepository">
    <select id="login" parameterType="BUser" resultType="BUser">
        select * from busertable where bname = #{bname} and bpwd = #{bpwd}
    </select>
    <insert id="saveUser" parameterType="BUser">
        insert into busertable(bname, bpwd) values (#{bname}, #{bpwd})
    </insert>
</mapper>